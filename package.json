{
  "name": "@alcalzone/esm2cjs",
  "publishConfig": {
    "access": "public"
  },
  "description": "Command line utility to compile a JS project from ES modules to CommonJS",
  "version": "1.1.0",
  "bin": "bin/esm2cjs.js",
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "exports": {
    ".": {
      "import": "./build/esm/index.js",
      "require": "./build/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "build/esm/index.d.ts",
  "typesVersions": {
    "*": {
      "build/esm/index.d.ts": [
        "build/esm/index.d.ts"
      ],
      "build/cjs/index.d.ts": [
        "build/esm/index.d.ts"
      ]
    }
  },
  "files": [
    "bin/",
    "build/**/*.{js,d.ts,map}"
  ],
  "repository": "https://github.com/AlCalzone/esm2cjs",
  "author": "Dominic Griesel <d.griesel@gmx.net>",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "node -r esbuild-register .postbuild.ts",
    "test:reset": "jest --clear-cache",
    "test": "jest",
    "test:watch": "jest --watch",
    "release": "release-script"
  },
  "dependencies": {
    "esbuild": "^0.13.8",
    "fs-extra": "^10.0.0",
    "tiny-glob": "^0.2.9",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^1.10.0",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@tsconfig/node14": "^1.0.1",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.1",
    "@types/yargs": "^17.0.4",
    "babel-jest": "^27.0.2",
    "esbuild-register": "^3.0.0",
    "jest": "^27.0.4",
    "typescript": "^4.3.2"
  }
}
